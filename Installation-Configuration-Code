DROP TABLESPACE SYSTEM INCLUDING CONTENTS AND DATAFILES
/

CREATE TABLESPACE APEX
DATAFILE 
  'C:\APP\ADMINISTRATOR\ORADATA\TT\APEX01.DBF' SIZE 1G AUTOEXTEND ON NEXT 50M MAXSIZE UNLIMITED
LOGGING
ONLINE
EXTENT MANAGEMENT LOCAL AUTOALLOCATE
BLOCKSIZE 8K
FLASHBACK ON
/

@apexins.sql APEX APEX TEMP /i/

SELECT comp_name, version, status FROM dba_registry WHERE comp_id='APEX';

SELECT * From dba_users WHERE TRUNC(CREATED) = TRUNC(SYSDATE);

SELECT * FROM apex_release;

